{
  "name": "eggxiaomi",
  "version": "1.0.0",
  "description": "",
  "private": true,
  "egg": {
    "typescript": true,
    "declarations": true
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production egg-scripts start --daemon --title=egg-server-eggxiaomi",
    "stop": "egg-scripts stop --title=egg-server-eggxiaomi",
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "test-local": "egg-bin test",
    "test": "npm run lint -- --fix && npm run test-local",
    "cov": "egg-bin cov",
    "tsc": "ets && tsc -p tsconfig.json",
    "ci": "npm run lint && npm run cov && npm run tsc",
    "autod": "autod",
    "lint": "tslint --fix --project . -c tslint.json",
    "clean": "ets clean",
    "build": "rimraf app/public/index && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config app/web/build/webpack.client.config.ts --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config app/web/build/webpack.server.config.ts --progress --hide-modules"
  },
  "dependencies": {
    "compression": "^1.7.4",
